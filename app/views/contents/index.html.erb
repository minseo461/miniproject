<h1>Contents#index</h1>
<p>Find me in app/views/contents/index.html.erb</p>

<%= link_to '새 글 쓰기', new_content_path %><br><br>


<% @contents.each do |content| %>
    <a href='<%= content_path(content)%>'><%= content.title %></a>
    <%= content.user.email %>
    <!-- #부모는 단수로 불러오고 자식은 복수로불러옴-->
    <%= link_to '글 수정하기', edit_content_path(content) %>
    <%= link_to '삭제하기', content_path(content), method: :delete, 
        data:{confirm:'정말 삭제하시겠습니까?'} %><br>
<%end%>